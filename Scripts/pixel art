bg1115t = Instance.new("TextButton", fxr)
bg1115t.Name = "b1"
bg1115t.Size = UDim2.new(0.0, 155, 0.0, 15)
bg1115t.Position = UDim2.new(0,100,0,8)
bg1115t.BackgroundColor3 = Color3.new(0.12,0.12,0.12)
bg1115t.BorderColor3 = Color3.new(0.12,0.12,0.12)
bg1115t.BackgroundTransparency = 0
bg1115t.TextColor3 = Color3.new(1,1,1)
bg1115t.Text = "Steal Selected Player's Art."
bg1115t.MouseButton1Down:connect(function () 
if bg1115.BackgroundColor3 == Color3.new(0.69,0.13,0.13) then
	game.ReplicatedStorage.Remote:InvokeServer("Save",Workspace.DrawBoards.DrawBoards["2"].Pixels,"Stolen_1")
	print("1")	
elseif b21s.BackgroundColor3 == Color3.new(0.69,0.13,0.13) then
	game.ReplicatedStorage.Remote:InvokeServer("Save",Workspace.DrawBoards.DrawBoards["6"].Pixels,"Stolen_6")
	print("6")
elseif b1115p.BackgroundColor3 == Color3.new(0.69,0.13,0.13) then
	game.ReplicatedStorage.Remote:InvokeServer("Save",Workspace.DrawBoards.DrawBoards["3"].Pixels,"Stolen_2")
	print("2")
elseif b121.BackgroundColor3 == Color3.new(0.69,0.13,0.13) then
	game.ReplicatedStorage.Remote:InvokeServer("Save",Workspace.DrawBoards.DrawBoards["5"].Pixels,"Stolen_4")
	print("4")
elseif b21.BackgroundColor3 == Color3.new(0.69,0.13,0.13) then
	game.ReplicatedStorage.Remote:InvokeServer("Save",Workspace.DrawBoards.DrawBoards["7"].Pixels,"Stolen_7")
	print("7")
elseif b111.BackgroundColor3 == Color3.new(0.69,0.13,0.13) then
	game.ReplicatedStorage.Remote:InvokeServer("Save",Workspace.DrawBoards.DrawBoards["4"].Pixels,"Stolen_3")
	print("3")
elseif b11.BackgroundColor3 == Color3.new(0.69,0.13,0.13) then
	game.ReplicatedStorage.Remote:InvokeServer("Save",Workspace.DrawBoards.DrawBoards["1"].Pixels,"Stolen_5")
	print("5")
elseif b1.BackgroundColor3 == Color3.new(0.69,0.13,0.13) then
	game.ReplicatedStorage.Remote:InvokeServer("Save",Workspace.DrawBoards.DrawBoards["8"].Pixels,"Stolen_8")
	print("8")
end
end)

