ar = 1
local r = game.Players.LocalPlayer 
Mouse = r:GetMouse() 
function keyb(key) 
	if (key == "e") then
		if ar == 1 then
			ar = 0
			fxr.Position = UDim2.new(99999,99999,99999,99999)
			fxr1.Position = UDim2.new(99999,99999,99999,99999)
		elseif ar == 0 then
			fxr.Position = UDim2.new(0.5,-270/2,0.5,-80/2)
			fxr1.Position = UDim2.new(0.5,-280/2,0.5,-80/2)
			ar = 1
		end
	 end 
end 
Mouse.KeyDown:connect(keyb)

    local hint = Instance.new("Hint",game.Players.LocalPlayer.PlayerGui)
    hint.Text = "Please read Console!(F9) to avoid over-writing a stolen art!"
    hint.Name = game.JobId
    wait(5)
    hint:Destroy()

print'PixelMaster -- DLLExploits'
print'-----------------------------'
print'To avoid overwriting saved art, or sizes, PLEASE move, or rename until a random artwork is at top of saves.'
print'Anything at the top is risk of over-write, and MAY be replaced forever'
print'Also select only ONE place to steal at a time, multiple will probably break everything'
print'If a new player joins, and you can not draw, just select the draw on others plot again'
print'v1.0(initial release)'
